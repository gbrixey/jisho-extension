{
    "manifest_version": 2,
    "name": "Jisho Extension",
    "version": "0.1",
    
    "browser_action": {
        "default_icon": "icon128.png",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs"
    ],
    "icons": {
        "16": "icon16.png",
        "32": "icon32.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}
